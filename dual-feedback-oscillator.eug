// Part properties

Property SEQUENCE(txt);

// Part types

PartType Promoter(SEQUENCE);
PartType RBS(SEQUENCE);
PartType CDS(SEQUENCE);
PartType Terminator(SEQUENCE);

// Parts

Promoter K094120("catagcatttttatccataagattagcggatctaacctttacaattgtgagcgctcacaattatgatagattcaattgtgagcggataacaatttcacacaga");

RBS B0034("aaagaggagaaa");

CDS C0012("");

CDS C0080("");

CDS K194001("");

Terminator B0015("ccaggcatcaaataaaacgaaaggctcagtcgaaagactgggcctttcgttttatctgttgtttgtcggtgaacgctctctactagagtcacactggctcaccttcgggtgggcctttctgcgtttata");

// Devices

Device DualControlRepressor(K094120, B0034, C0012, B0015);

Device DualControlActivator(K094120, B0034, C0080, B0015);

Device DualControlReporter(K094120, B0034, K194001, B0015);

Device DualFeedbackOscillator(+DualControlRepressor, DualControlActivator, DualControlReporter);

Rule DualFeedbackOscillatorAlternating(ON DualFeedbackOscillator:
	ALTERNATE_ORIENTATION
);

// Enumerate devices

result = permute(DualFeedbackOscillator);

// Print devices

println(SIZEOF(result));

println(result);

// Visualize devices

for(num i = 0; i < SIZEOF(result); i = i + 1) {
   SBOL.visualize(result[i]);
}

// Export devices to SBOL

SBOL.export(result, "./sbol/DualFeedbackOscillators.xml");

